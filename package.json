{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Node, express and MongoDB",
  "main": "app.js",
  "renameFile": "./public/js/bundle.js",
  "targets": {
    "renameFile": {
      "outputFormat": "global",
      "includeNodeModules": true,
      "sourceMap": false,
      "source": "./public/js/index.js",
      "distDir": "./public/js"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production & nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js --target bundle.js"
  },
  "author": "Pavneet Singh",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.22",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "pug": "^3.0.3",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stripe": "^17.4.0",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "ndb": "^1.1.5",
    "nodemon": "^3.1.0",
    "parcel": "^2.13.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10"
  }
}
